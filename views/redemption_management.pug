extends layout

block styles
    link(rel='stylesheet', href='/css/user_maintenance.css')
    link(rel='stylesheet', href='/css/table.css')
    link(rel='stylesheet', href='/css/coming_soon.css')
    script(src="/js/profile.js")

block header
    .title-bar
        .title-bar__title
            h1 Redemption Management
            span.breadcrumbs Dashboard > #{page}
        .title-bar__profile
            .profile-info
                span#UserName
                br
                span#UserEmail
            .profile-frame
                img(src='/img/logo/profile.png')

block content
    .coming-soon-container
        h1.coming-soon-title Coming Soon
        p.coming-soon-subtitle We're working hard to bring you something amazing. Stay tuned!

